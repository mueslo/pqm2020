   "source": [
    "def hamiltonian(t, delta):\n",
    "    return np.array([[0, t], [t, delta]])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "def solve(t, delta):\n",
    "    H = hamiltonian(t, delta)\n",
    "    return np.linalg.eigvals(H)"
   ]
  },
